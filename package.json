{
  "name": "employee-management-system",
  "version": "0.0.1",
  "description": "",
  "author": "Yannick Zahinda",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:ci": "jest --coverage --maxWorkers=2",
    "docker:build": "docker build -t employee-management-system .",
    "docker:run": "docker-compose up",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts -n",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register scripts/seed-admin.ts",
    "db:setup": "npm run build && npx typeorm migration:run -d dist/src/database/data-source.js && npm run seed:admin"

  },
